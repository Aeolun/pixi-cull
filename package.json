{
    "name": "pixi-cull",
    "version": "2.0.0",
    "description": "a library to visibly cull objects designed to work with pixi.js",
    "main": "dist/index.js",
    "module": "code/index.js",
    "types": "@types/index.d.ts",
    "scripts": {
        "start": "rollup -c rollup.dev.js --watch",
        "tsc": "tsc --noEmit --module amd --moduleResolution node --skipLibCheck code/index.ts",
        "declaration": "tsc --declaration --declarationDir @types --emitDeclarationOnly code/index.ts",
        "docs": "jsdoc -c .jsdoc.json",
        "prepublishOnly": "yarn transpile && yarn build-demo && yarn bundle && yarn docs"
    },
    "keywords": [
        "pixi.js",
        "cull",
        "pixi-viewport",
        "viewport",
        "spatial",
        "hash",
        "simple",
        "cull"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidfig/pixi-cull.git"
    },
    "bugs": {
        "url": "https://github.com/davidfig/pixi-cull/issues"
    },
    "homepage": "https://github.com/davidfig/pixi-cull#readme",
    "author": "David S. Figatner",
    "license": "MIT",
    "peerDependencies": {
        "pixi.js": "^6.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@rollup/plugin-typescript": "^8.2.0",
        "rollup-plugin-serve": "^1.1.0",
        "fork-me-github": "^1.2.0",
        "jsdoc": "^3.6.5",
        "pixi-viewport": "^4.13.2",
        "pixi.js": "6.0.1",
        "rollup": "^2.44.0",
        "typescript": "^4.2.3",
        "yy-fps": "^1.1.0",
        "yy-jsdoc-template": "^1.3.0",
        "yy-random": "^1.8.0"
    }
}
