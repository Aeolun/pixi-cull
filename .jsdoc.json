{
    "applicationName": "pixi-cull",
    "disqus": "",
    "openGraph": {
        "title": "",
        "type": "website",
        "image": "",
        "site_name": "",
        "url": ""
    },
    "meta": {
        "title": "pixi-cull",
        "description": "",
        "keyword": ""
    },
    "linenums": true,
    "source": {
        "include": [
            "./code/index.ts",
            "./code/types.ts",
            "./code/simple.ts",
            "./code/spatial-hash.ts"
        ],
        "includePattern": ".+\\.ts(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "readme": "./README.md",
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs/jsdoc",
        "template": "./node_modules/yy-jsdoc-template"
    },
    "plugins": [
        "jsdoc-plugin-typescript",
        "./node_modules/yy-jsdoc-template/plugins/skip-node_modules",
        "plugins/markdown"
    ],
    "typescript": {
        "moduleRoot": "code"
    },
    "templates": {
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "pixi-cull",
        "footer"        : "by YOPEY YOPEY LLC (yopeyopey.com)",
        "copyright"     : "Copyright Â© 2018 YOPEY YOPEY LLC.",
        "meta": {
            "title": "pixi-cull API Documentation",
            "description": "Documentation for pixi-cull library",
            "keyword": "docs, documentation, html5, javascript, jsdoc, viewport, cull, bounce, 2d"
        },
        "matomo": {
            "url": "https://analytics.yopeyopey.com/piwik/",
            "id": 22
        }
    }
