{
  "source": {
    "include": "code/",
    "excludePattern": "(node_modules|lib|test)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "docs/jsdoc"
  },
  "template": {
    "readme": "./README.md",
    "applicationName": "pixi-cull",
    "repository": "https://github.com/davidfig/pixi-cull/",
    "outputSourceFiles": true
  }
}